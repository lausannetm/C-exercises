#include<iostream>
using namespace std;
int main()
{
	int a;
	cin >> a;
	
	int a1 = (a / 10000000) % 10;
	int a2 = (a / 1000000) % 10;
	int a3 = (a / 100000) % 10;
	int a4 = (a / 10000) % 10;
	int a5 = (a / 1000) % 10;
	int a6 = (a / 100) % 10;
	int a7 = (a / 10) % 10;
	int a8 = (a / 1) % 10;

	
	float avr = (a1 + a2 + a3 + a4 + a5 + a6 + a7 + a8) / 8.0;

	cout << avr << endl;

	return 0;
}
